<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>piracer</name>
  <version>0.0.0</version>
  <description>ROS2 package for Waveshare PiRacer</description>
  <maintainer email="morrissettal2@vcu.edu">Adam Morrissett</maintainer>
  <license>TODO: License declaration</license>

  <depend>autonomy_interfaces</depend>
  <depend>command_msgs</depend>
  <depend>piracer_msgs</depend>
  <depend>rclcpy</depend>
  <depend>std_msgs</depend>
